  "scripts": {
    "dev": "concurrently \"yarn dev:renderer\" \"yarn electron:serve\" --kill-others",
    "dev:main": "cross-env NODE_ENV=development webpack --config _config/webpack/webpack.main.js --mode development && yarn electron:run",
    "dev:renderer": "cross-env NODE_ENV=development webpack-dev-server --config _config/webpack/webpack.react.js --mode development",
    "electron:serve": "wait-on http://localhost:4000 && yarn dev:main",
    "electron:run": "electron ."
  },
  "devDependencies": {
    "@babel/preset-react": "^7.10.4",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.2.1",
    "electron": "^9.2.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.3.0",
    "eslint-plugin-react-hooks": "4",
    "html-webpack-plugin": "^4.3.0",
    "style-loader": "^1.2.1",
    "typescript": "^3.9.7",
    "wait-on": "^5.2.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "core-js": "^3.6.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.7"
  }
}
